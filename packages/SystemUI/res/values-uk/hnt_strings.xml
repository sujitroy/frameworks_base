<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Network traffic monitor string format -->
    <string name="network_traffic_format"><xliff:g id="value">%1$s</xliff:g> <xliff:g id="unit" example="kB">%2$s</xliff:g>/с</string>

    <!-- Recents memory bar -->
    <string name="recents_free_ram">Вільно пам\'яті: %1$d МБ</string>

    <!-- Reboot/Recovery QS Tile -->
    <string name="quick_settings_reboot_label">Перезавантаження</string>
    <string name="quick_settings_reboot_recovery_label">В рекавері</string>

    <!-- Perf Profile Tile -->
    <string name="qs_tile_performance">Режим продуктивності</string>
    <!-- Content description of the battery mode tile in quick settings when on, power save mode (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_perf_profile_pwrsv">Режим продуктивності: економія енергії.</string>
    <!-- Content description of the battery mode tile in quick settings when on, balanced mode (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_perf_profile_bal">Режим продуктивності: збалансований.</string>
    <!-- Content description of the battery mode tile in quick settings when on, performance mode (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_perf_profile_perf">Режим продуктивності: продуктивність.</string>
    <!-- Content description of the battery mode tile in quick settings when on, efficiency mode (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_perf_profile_bias_power">Режим продуктивності: ефективність.</string>
