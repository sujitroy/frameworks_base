    <string name="accessibility_quick_settings_perf_profile_changed_bias_power">Режим продуктивності змінено на ефективний.</string>
    <!-- Announcement made when the battery mode tile changes to quick (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_perf_profile_changed_bias_perf">Режим продуктивності змінено на швидкий.</string>
    <string name="quick_settings_performance_profile_detail_title">Режим продуктивності</string>
